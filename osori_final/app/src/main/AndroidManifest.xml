<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.template.sjl95.osori_final">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="Osori"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:screenOrientation="portrait"
        >

        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ItemSearchActivity" />
        <activity android:name=".ItemSelectActivity" />
        <activity android:name=".StoreSearchActivity" />
        <activity android:name=".StoreSelectActivity"></activity>
    </application>

</manifest>